@model List<SaveSeason>



<div class="col-12">
	<div class="dashbox">
		<div class="dashbox__title">
			<h3><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M12,6a1,1,0,0,0-1,1V17a1,1,0,0,0,2,0V7A1,1,0,0,0,12,6ZM7,12a1,1,0,0,0-1,1v4a1,1,0,0,0,2,0V13A1,1,0,0,0,7,12Zm10-2a1,1,0,0,0-1,1v6a1,1,0,0,0,2,0V11A1,1,0,0,0,17,10Zm2-8H5A3,3,0,0,0,2,5V19a3,3,0,0,0,3,3H19a3,3,0,0,0,3-3V5A3,3,0,0,0,19,2Zm1,17a1,1,0,0,1-1,1H5a1,1,0,0,1-1-1V5A1,1,0,0,1,5,4H19a1,1,0,0,1,1,1Z" /></svg>Сохраненый сезон и серия</h3>
		</div>

		<div class="dashbox__table-wrap dashbox__table-wrap--1">
			<table class="main__table main__table--dash">
				<thead>
					<tr>
						<th>Наименование</th>
						<th>Сезон</th>
						<th>Серия</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var order in Model)
					{
						<tr>
							<td>
								<div class="main__table-text">@order.PrimaryTitle</div>
							</td>
							<td>
								<div class="main__table-text">@order.Season</div>
							</td>
							<td>
								<div class="main__table-text">@order.Seriya</div>
							</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>
</div>
